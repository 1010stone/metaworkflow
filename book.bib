
@article{loftfield_comparison_2016,
  title = {Comparison of {{Collection Methods}} for {{Fecal Samples}} for {{Discovery Metabolomics}} in {{Epidemiologic Studies}}},
  volume = {25},
  copyright = {\textcopyright{}2016 American Association for Cancer Research.},
  issn = {1055-9965, 1538-7755},
  doi = {10.1158/1055-9965.EPI-16-0409},
  abstract = {Background: The gut metabolome may be associated with the incidence and progression of numerous diseases. The composition of the gut metabolome can be captured by measuring metabolite levels in the feces. However, there are little data describing the effect of fecal sample collection methods on metabolomic measures.
Methods: We collected fecal samples from 18 volunteers using four methods: no solution, 95\% ethanol, fecal occult blood test (FOBT) cards, and fecal immunochemical test (FIT). One set of samples was frozen after collection (day 0), and for 95\% ethanol, FOBT, and FIT, a second set was frozen after 96 hours at room temperature. We evaluated (i) technical reproducibility within sample replicates, (ii) stability after 96 hours at room temperature for 95\% ethanol, FOBT, and FIT, and (iii) concordance of metabolite measures with the putative ``gold standard,'' day 0 samples without solution.
Results: Intraclass correlation coefficients (ICC) estimating technical reproducibility were high for replicate samples for each collection method. ICCs estimating stability at room temperature were high for 95\% ethanol and FOBT (median ICC $>$ 0.87) but not FIT (median ICC = 0.52). Similarly, Spearman correlation coefficients (rs) estimating metabolite concordance with the ``gold standard'' were higher for 95\% ethanol (median rs = 0.82) and FOBT (median rs = 0.70) than for FIT (median rs = 0.40).
Conclusions: Metabolomic measurements appear reproducible and stable in fecal samples collected with 95\% ethanol or FOBT. Concordance with the ``gold standard'' is highest with 95\% ethanol and acceptable with FOBT.
Impact: Future epidemiologic studies should collect feces using 95\% ethanol or FOBT if interested in studying fecal metabolomics. Cancer Epidemiol Biomarkers Prev; 25(11); 1483\textendash{}90. \textcopyright{}2016 AACR.},
  language = {en},
  timestamp = {2016-12-05T14:43:26Z},
  number = {11},
  urldate = {2016-12-05},
  journal = {Cancer Epidemiology and Prevention Biomarkers},
  author = {Loftfield, Erikka and Vogtmann, Emily and Sampson, Joshua N. and Moore, Steven C. and Nelson, Heidi and Knight, Rob and Chia, Nicholas and Sinha, Rashmi},
  month = nov,
  year = {2016},
  note = {00000},
  pages = {1483--1490},
  file = {Snapshot:C\:\\Users\\SPMESUPERCPU\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\bxqqk9lp.default\\zotero\\storage\\R37VHHVG\\1483.html:text/html},
  pmid = {27543620}
}

@article{gomez-rios_biocompatible_2016,
  title = {Biocompatible {{Solid}}-{{Phase Microextraction Nanoelectrospray Ionization}}: {{An Unexploited Tool}} in {{Bioanalysis}}},
  volume = {88},
  issn = {0003-2700},
  shorttitle = {Biocompatible {{Solid}}-{{Phase Microextraction Nanoelectrospray Ionization}}},
  doi = {10.1021/acs.analchem.5b03668},
  abstract = {In recent years, different geometrical configurations of solid-phase microextraction (SPME) have been directly coupled to mass spectrometry, resulting in benefits such as diminishing matrix effects, improvement of detection limits, and considerable enhancement of analysis throughput. Although SPME fibers have been used for years, their potential for quantitative analysis when directly combined with mass spectrometry has not been explored to its full extent. In this study, we present the direct coupling of biocompatible SPME (Bio-SPME) fibers to mass spectrometry via nanoelectrospray ionization (nano-ESI) emitters as a powerful tool for fast quantitative analysis of target analytes in biofluids. Total sample preparation time does not exceed 2 min, and by selecting an appropriate fiber length and sample vessel, sample volumes ranging between 10 and 1500 $\mu$L can be used. Despite the short extraction time of the technique, limits of detection in the subnanogram per milliliter with good accuracy ($\geq$90\%) and linearity (R2 $>$ 0.999) were attained for all the studied probes in phosphate-buffered saline (PBS), urine, and whole blood. Given that Bio-SPME\textendash{}nano-ESI efficiently integrates sampling with analyte extraction/enrichment, sample cleanup (including elimination of matrix effects in the form of particles), and ionization, our results demonstrated that it is an advantageous configuration for bioanalytical applications such as therapeutic drug monitoring, doping in sports, and pharmacological studies in various matrixes.},
  timestamp = {2016-10-28T14:31:41Z},
  number = {2},
  urldate = {2016-10-27},
  journal = {Analytical Chemistry},
  author = {G{\'o}mez-R{\'\i}os, Germ{\'a}n Augusto and Reyes-Garc{\'e}s, Nathaly and Bojko, Barbara and Pawliszyn, Janusz},
  month = jan,
  year = {2016},
  note = {00008},
  pages = {1259--1265},
  file = {SPMEnanoESI_manuscript_081215_clear.doc:C\:\\Users\\SPMESUPERCPU\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\bxqqk9lp.default\\zotero\\storage\\EQ7W2RUI\\SPMEnanoESI_manuscript_081215_clear.doc:application/msword;ACS Full Text PDF w/ Links:C\:\\Users\\SPMESUPERCPU\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\bxqqk9lp.default\\zotero\\storage\\N64R3MIK\\Gómez-Ríos et al. - 2016 - Biocompatible Solid-Phase Microextraction Nanoelec.pdf:application/pdf;ACS Full Text Snapshot:C\:\\Users\\SPMESUPERCPU\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\bxqqk9lp.default\\zotero\\storage\\G2Z26W3U\\acs.analchem.html:text/html}
}

