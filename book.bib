  pages = {1266--1274},
  file = {invivo_pawliszyn_supporting_information.docx:C\:\\Users\\SPMESUPERCPU\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\bxqqk9lp.default\\zotero\\storage\\GDJRPTZF\\invivo_pawliszyn_supporting_information.docx:application/vnd.openxmlformats-officedocument.wordprocessingml.document;ACS Full Text PDF w/ Links:C\:\\Users\\SPMESUPERCPU\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\bxqqk9lp.default\\zotero\\storage\\KKCV9UKS\\Risticevic et al. - 2016 - Capturing Plant Metabolome with Direct-Immersion i.pdf:application/pdf;invivo_pawliszyn_manuscript_revision.docx:C\:\\Users\\SPMESUPERCPU\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\bxqqk9lp.default\\zotero\\storage\\UPVJZ74A\\invivo_pawliszyn_manuscript_revision.docx:application/vnd.openxmlformats-officedocument.wordprocessingml.document;ACS Full Text Snapshot:C\:\\Users\\SPMESUPERCPU\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\bxqqk9lp.default\\zotero\\storage\\WFVWEGZD\\acs.analchem.html:text/html}
