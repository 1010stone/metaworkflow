  file = {Supporting Information_ac-2014-015673.jp.docx:C\:\\Users\\SPMESUPERCPU\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\bxqqk9lp.default\\zotero\\storage\\CCFI63X7\\Supporting Information_ac-2014-015673.jp.docx:application/vnd.openxmlformats-officedocument.wordprocessingml.document;ACS Full Text PDF w/ Links:C\:\\Users\\SPMESUPERCPU\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\bxqqk9lp.default\\zotero\\storage\\NZEJXX8J\\Boyacı and Pawliszyn - 2014 - Micelle Assisted Thin-Film Solid Phase Microextrac.pdf:application/pdf;ac-2014-015673Rev2.jp.docx:C\:\\Users\\SPMESUPERCPU\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\bxqqk9lp.default\\zotero\\storage\\QW8XKBTS\\ac-2014-015673Rev2.jp.docx:application/vnd.openxmlformats-officedocument.wordprocessingml.document;ACS Full Text Snapshot:C\:\\Users\\SPMESUPERCPU\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\bxqqk9lp.default\\zotero\\storage\\8Q2I2TEZ\\ac5015673.html:text/html}
