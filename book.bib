  timestamp = {2016-12-05T14:13:48Z},
  number = {suppl 2},
  urldate = {2016-09-05},
  journal = {Nucleic Acids Research},
  author = {Xia, Jianguo and Psychogios, Nick and Young, Nelson and Wishart, David S.},
  month = jan,
  year = {2009},
  note = {00582},
  pages = {W652--W660},
  file = {Xia et al_2009_MetaboAnalyst.pdf:C\:\\Users\\SPMESUPERCPU\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\bxqqk9lp.default\\zotero\\storage\\XW5BEDWE\\Xia et al_2009_MetaboAnalyst.pdf:application/pdf;Snapshot:C\:\\Users\\SPMESUPERCPU\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\bxqqk9lp.default\\zotero\\storage\\PTZA6KWU\\W652.html:text/html},
  pmid = {19429898}
}

