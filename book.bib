  file = {Saghatelian et al_2004_Assignment of Endogenous Substrates to Enzymes by Global Metabolite Profiling.pdf:C\:\\Users\\SPMESUPERCPU\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\bxqqk9lp.default\\zotero\\storage\\SRVUJHAS\\Saghatelian et al_2004_Assignment of Endogenous Substrates to Enzymes by Global Metabolite Profiling.pdf:application/pdf;ACS Full Text Snapshot:C\:\\Users\\SPMESUPERCPU\\AppData\\Roaming\\Zotero\\Zotero\\Profiles\\bxqqk9lp.default\\zotero\\storage\\U37C663H\\bi0480335.html:text/html}
